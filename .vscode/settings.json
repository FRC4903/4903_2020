{
  "java.configuration.updateBuildConfiguration": "disabled",
  "java.import.gradle.enabled": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "bin/": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "files.associations": {
    "iostream": "cpp",
    "ostream": "cpp",
    "chrono": "cpp",
    "limits": "cpp",
    "random": "cpp",
    "cmath": "cpp"
  },
  "C_Cpp.default.configurationProvider": "vscode-wpilib"
}
